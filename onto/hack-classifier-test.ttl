hack:EquipmentCategory_Classifier
  rdf:type hack:BayesClassifier ;
  rdfs:label "Категория оборудования"@ru ;
  rdfs:label "Equipment category"@en ;
  hack:hasBayesCategory hack:EquipmentCategory1 ;
  hack:hasBayesCategory hack:EquipmentCategory2 ;
  hack:hasBayesCategory hack:EquipmentCategory3 ;
  hack:hasBayesCategory hack:EquipmentCategory4 ;
  hack:hasBayesCategory hack:EquipmentCategory5 ;
  hack:hasBayesCategory hack:EquipmentCategory6 ;
  hack:hasBayesCategory hack:EquipmentCategory7 ;
  hack:hasBayesCategory hack:EquipmentCategory8 ;
  hack:hasBayesCategory hack:EquipmentCategory9 ;
  hack:hasBayesCategory hack:EquipmentCategory10 ; 
  hack:hasBayesCategory hack:EquipmentCategory11 ;     
  hack:forClass hack:EquipmentCategory ;
.

hack:BayesSpecification_EquipmentCategory
  rdf:type hack:BayesSpecification ;
  hack:hasBayesClassifier hack:EquipmentCategory_Classifier ;
  hack:forClass hack:Request ;
  hack:sourceProperty v-s:content ;
  hack:targetProperty hack:hasEquipmentCategory ;
.

# ---------- Классификатор аварийности


hack:FailureRate1
  rdf:type hack:BayesCategory ;
  v-s:parent hack:FailureRate_Classifier ;
  rdf:value hack:failure ;
  hack:keyPhrases """авария
прорыв
разрыв
срыв
взрыв
авария
возгорание
воспламенение
отключение
обрушение
залп
хлещет
заливает
сгорел""" ;
.

hack:FailureRate2
  rdf:type hack:BayesCategory ;
  v-s:parent hack:FailureRate_Classifier ;
  rdf:value hack:ongoingService ;
  hack:keyPhrases """не авария
сгорела лампочка
ура
привет
мама""" ;
.

hack:FailureRate_Classifier
  rdf:type hack:BayesClassifier ;
  rdfs:label "Классификатор аварийности"@ru ;
  rdfs:label "Failure rate classifier"@en ;
  hack:hasBayesCategory hack:FailureRate1 ;
  hack:hasBayesCategory hack:FailureRate2 ;
  hack:forClass hack:FailureRate ;
.


hack:BayesSpecification_FailureRate
  rdf:type hack:BayesSpecification ;
  hack:hasBayesClassifier hack:FailureRate_Classifier ;
  hack:forClass hack:Request ;
  hack:sourceProperty v-s:content ;
  hack:targetProperty hack:hasFailureRate ;
.

