<http://semantic-machines.com/veda/hack-classifier-test>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология. Обращения. Классификатор. Тестовые данные"@ru ;
  rdfs:label "Ontology. Request. Сlassifier. Test data"@en ;
  v-s:loadPriority 20 ;
.

# ---------- Классификатор категории оборудования

hack:EquipmentCategory_Classifier
  rdf:type hack:BayesClassifier ;
  rdfs:label "Категория оборудования"@ru ;
  rdfs:label "Equipment category"@en ;
  hack:hasBayesCategory hack:EquipmentCategory1 ;
  hack:hasBayesCategory hack:EquipmentCategory2 ;
  hack:hasBayesCategory hack:EquipmentCategory3 ;
  hack:forClass hack:EquipmentCategory ;
.

hack:EquipmentCategory1
  rdf:type hack:BayesCategory ;
  v-s:parent hack:EquipmentCategory_Classifier ;
  rdf:value hack:coldWaterSupply ;
  hack:keyPhrases """труба
кран
течь в подвале
холодная вода
течь смесителя в ванной""" ;
.

hack:EquipmentCategory2
  rdf:type hack:BayesCategory ;
  v-s:parent hack:EquipmentCategory_Classifier ;
  rdf:value hack:lifting ;
  hack:keyPhrases """лифт
вибрация кабины
не совпадает пол кабины и этажа
освещение в лифте
сожжены кнопки в лифте""" ;
.

hack:EquipmentCategory3
  rdf:type hack:BayesCategory ;
  v-s:parent hack:EquipmentCategory_Classifier ;
  rdf:value hack:gas ;
  hack:keyPhrases """прорыв газа
прекращена подача газа,
запах газа в подъезде
запах газа в квартире
газовая труба
газопровод""" ;
.

hack:BayesSpecification_EquipmentCategory
  rdf:type hack:BayesSpecification ;
  hack:hasBayesClassifier hack:EquipmentCategory_Classifier ;
  hack:forClass hack:Request ;
  hack:sourceProperty v-s:content ;
  hack:targetProperty hack:hasEquipmentCategory ;
.

# ---------- Классификатор аварийности

hack:FailureRate_Classifier
  rdf:type hack:BayesClassifier ;
  rdfs:label "Классификатор аварийности"@ru ;
  rdfs:label "Failure rate classifier"@en ;
  hack:hasBayesCategory hack:FailureRate1 ;
  hack:hasBayesCategory hack:FailureRate2 ;
  hack:forClass hack:FailureRate ;
.

hack:FailureRate1
  rdf:type hack:BayesCategory ;
  v-s:parent hack:FailureRate_Classifier ;
  rdf:value hack:failure ;
  hack:keyPhrases """авария
прорыв
разрыв
срыв
взрыв
авария
возгорание
воспламенение
отключение
обрушение
залп
хлещет
заливает
сгорел дом""" ;
.

hack:FailureRate2
  rdf:type hack:BayesCategory ;
  v-s:parent hack:FailureRate_Classifier ;
  rdf:value hack:ongoingService ;
  hack:keyPhrases """не авария
сгорела лампочка
ура
привет
мама""" ;
.

hack:BayesSpecification_FailureRate
  rdf:type hack:BayesSpecification ;
  hack:hasBayesClassifier hack:FailureRate_Classifier ;
  hack:forClass hack:Request ;
  hack:sourceProperty v-s:content ;
  hack:targetProperty hack:hasFailureRate ;
.

# ---------------------- Тестовая заявка

d:Request1
  rdf:type hack:Request ;
  v-s:content "У меня не работает лифт в подъезде." ;
.

d:Request1_classified
  rdf:type hack:Request ;
  v-s:content "У меня не работает лифт в подъезде." ;
  hack:hasEquipmentCategory hack:lifting ;
  hack:hasFailureRate hack:ongoingService ;
  hack:isClassified true ;
.
