<http://semantic-machines.com/veda/hack-request-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология. Обращения. Схема"@ru ;
  rdfs:label "Ontology. Request. Schema"@en ;
  v-s:loadPriority 20 ;
.

#-------------------classes

#ОБРАЩЕНИЕ
hack:Request
	rdf:type owl:Class ;
	rdfs:subClassOf v-s:UserThing ;
	rdfs:subClassOf v-s:Request ;
	rdfs:subClassOf v-s:Labeled ;
	rdfs:label "Обращение"@ru ;
	rdfs:label "Request"@en ;
	v-s:labelPattern "{@.v-s:registrationNumber} {@.v-s:hasStatus.rdfs:label}" ;
	v-ui:hasTemplate hack:RequestTemplate ;
.

#rdfs:label
#v-s:registrationNumber
#v-s:registrationDate
#v-s:content
#v-s:hasStatus
#hack:hasCategory
#v-s:hasPriorityLevel
#v-s:hasPersonalAccount
#метод доставки (как получили + коммент email)
#hack:isPlanned
#hack:isFailure
#hack:hasObject
#v-s:hasDelivery (как получено)
#v-s:hasComment (отчет об исполнении)

#Лицевой счет/договор
hack:PersonalAccount
	rdf:type owl:Class ;
	rdfs:subClassOf v-s:UserThing ;
	rdfs:subClassOf v-s:Labeled ;
	rdfs:label "Лицевой счет"@ru ;
	rdfs:label "PersonalAccount"@en ;
	v-s:labelPattern "{@.v-s:registrationNumber} {@.v-s:hasStatus.rdfs:label}" ;
	v-ui:hasTemplate hack:PersonalAccountTemplate ;
.
#rdfs:label
#v-s:registrationNumber
#v-s:customer
#hack:hasObject
#v-s:hasCommunicationMean


#Категория обрудования и сооружений
hack:EquipmentCategory
	rdf:type owl:Class ;
	rdfs:subClassOf v-s:UserThing ;
	rdfs:label "Категория оборудования и сооружений"@ru ;
	rdfs:label "Equipment category"@en ;
	v-ui:hasTemplate hack:EquipmentCategoryTemplate ;
.
