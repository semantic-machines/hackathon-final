@prefix d: <http://semantic-machines.com/veda/veda-data/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-wf: <http://semantic-machines.com/veda/veda-workflow/> .
@prefix cfg: <http://semantic-machines.com/veda/config/> .
@prefix v-fc: <http://semantic-machines.com/veda/veda-function-create/> .
@prefix hack: <http://semantic-machines.com/veda/hack-schema/> .

<http://semantic-machines.com/veda/hack-request-ui>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология. Обрашение. UI"@ru ;
  rdfs:label "Ontology. Request. UI"@en ;
  v-s:loadPriority 20 ;
.
#-------
hack:ReportBundle
  rdf:type v-s:Bundle ;
  rdfs:label "Отчет об исполнении"@ru ;
  rdfs:label "Report"@en ;
.

#---------------------------------------------------------
hack:RequestBlank
  a v-fc:Blank ;
  rdfs:label "Обращение"@ru ;
  rdfs:label "Request"@en ;
  v-fc:targetType hack:Request ;
.

hack:RequestTemplate
  rdf:type v-ui:ClassTemplate ;
  v-ui:forClass hack:Request ;
  rdfs:label "Template for hack:Request class"@en ;
  rdfs:label "Шаблон для класса hack:Request"@ru ;
  v-ui:template """
<div>
  <div class="container sheet">
    <section id="MainProperties">
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="v-s:registrationNumber" property="rdfs:label"></label>
        </div>
        <div class="col-sm-3 col-xs-3">
          <div property="v-s:registrationNumber" class="view -edit search"></div>
          <veda-control property="v-s:registrationNumber" data-type="string" class="-view edit search"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="v-s:registrationDate" property="rdfs:label"></label>
        </div>
        <div class="col-sm-3 col-xs-3">
          <div property="v-s:registrationDate" class="view -edit search"></div>
          <veda-control property="v-s:registrationDate" data-type="date" class="-view edit search"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="v-s:content" property="rdfs:label"></label>
        </div>
        <div class="col-sm-9 col-xs-7">
          <div property="v-s:content" class="view -edit search"></div>
          <veda-control property="v-s:content" data-type="text" class="-view edit search"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="v-s:hasStatus" property="rdfs:label"></label>
        </div>
        <div class="col-sm-9 col-xs-7">
          <div rel="v-s:hasStatus" class="view -edit search" data-template="v-ui:LabelTemplate"></div>
          <veda-control data-type="link" rel="v-s:hasStatus" class="-view edit search fulltext dropdown"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="hack:hasObject" property="rdfs:label"></label>
        </div>
        <div class="col-sm-9 col-xs-7">
          <div rel="hack:hasObject" class="view -edit search" data-template="v-ui:LabelTemplate"></div>
          <veda-control data-type="link" rel="hack:hasObject" class="-view edit search fulltext"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5"/>
        <div class="col-sm-9 col-xs-7">
          <strong about="v-s:isPlanned" property="rdfs:label"></strong>
          <veda-control property="v-s:isPlanned" data-type="booleanRadio"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5"/>
        <div class="col-sm-9 col-xs-7">
          <strong about="v-s:isFailure" property="rdfs:label"></strong>
          <veda-control property="v-s:isFailure" data-type="booleanRadio"></veda-control>
        </div>
      </div>
      <div class="row row-attribute">
        <div class="col-sm-3 col-xs-5">
          <label about="v-s:hasDelivery" property="rdfs:label"></label>
        </div>
        <div class="col-sm-9 col-xs-7">
          <div rel="v-s:hasDelivery" class="view edit search" data-template="v-s:DeliveryEmbeddedTemplate"></div>
          <veda-control data-type="link" rel="v-s:hasDelivery" class="-view edit search"></veda-control>
        </div>
      </div>
    </section>
    <section id="Report">
      <h4 class="section-header clearfix">
        <span about="hack:ReportBundle" property="rdfs:label"></span>
        <button class="btn btn-xs btn-success pull-right" id="add-report">
          <span class="glyphicon glyphicon-search"></span>
          <span about="hack:ReportBundle" property="rdfs:label"></span>
        </button>
      </h4>
      <div rel="v-s:hasComment" data-template="hack:CommentTemplate" data-embedded="true"></div>      
    </section>  
  </div>
</div>
<script>

</script>
"""
.
