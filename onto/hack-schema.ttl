@prefix d: <http://semantic-machines.com/veda/veda-data/> .
@prefix hack: <http://semantic-machines.com/veda/hack-schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v-s: <http://semantic-machines.com/veda/veda-schema/> .
@prefix v-ui: <http://semantic-machines.com/veda/veda-ui/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix v-wf: <http://semantic-machines.com/veda/veda-workflow/> .
@prefix cfg: <http://semantic-machines.com/veda/config/> .
@prefix v-fc: <http://semantic-machines.com/veda/veda-function-create/> .
@prefix v-df: <http://semantic-machines.com/veda/veda-docflow/> .

<http://semantic-machines.com/veda/hack-request-schema>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология ОБЩИЕ РЕШЕНИЯ. Запросы. Схема"@ru ;
  rdfs:label "Gen ontology. Request. Schema"@en ;
  v-s:loadPriority 20 ;
.
# ------------------------------------------------------------ КЛАССЫ --

hack:Request
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:Request ;
  rdfs:label "Заявка"@ru ;
  rdfs:label "Request"@en ;
.

#-------------------------------------------------------------- Запрос------
hack:TS_Request
  rdf:type v-ui:TemplateSpecification ;
  v-ui:forClass hack:Request ;
  v-ui:defaultTemplate hack:RequestTemplate ;
  v-s:loadPriority 10 ;
.

#-----Регистрационный номер
#v-s:registrationNumber
hack:PS_registrationNumber_Request
	rdf:type v-ui:DatatypePropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:registrationNumber ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 0 ;
	v-ui:tooltip "Присваивается автоматически"@ru ;
	v-ui:tooltip "Assigned automatically"@en ;
	rdfs:label "Спецификация v-s:registrationNumber  для класса hack:Request"@ru ;
	rdfs:label "v-s:registrationNumber property specification for hack:Request class"@en ;
.

#-----Статус инновации
#v-s:hasStatus
hack:PS_hasStatus_Request
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:hasStatus ;
	v-ui:maxCardinality 30 ;
	v-ui:minCardinality 1 ;
	v-ui:alternativeName "Статус инновации"@ru ;
	v-ui:defaultValue v-s:StatusDraft ;
	#v-ui:queryPrefix "('rdf:type'=='v-s:Status') && ('@' == 'v-s:StatusAgreed' || '@' == 'v-s:StatusArchived' || '@' == 'v-s:StatusImplementation' || '@' == 'v-s:StatusPostponed' )"^^xsd:string ;
	v-ui:tooltip "Выберите из списка."@ru ;
	v-ui:tooltip "Select from list"@en ;
	rdfs:label "Спецификация v-s:hasStatus для hack:Request"@ru ;
	rdfs:label "v-s:hasStatus property specification for hack:Request class"@en ;
.

# v-s:title
hack:PS_title_Request
	rdf:type v-ui:DatatypePropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:title ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 1 ;
	rdfs:label "Спецификация v-s:title для hack:Request"@ru ;
	rdfs:label "v-s:title property specification for hack:Request class"@en ;
.

#-----Описание актуального состояния
# v-s:actualState
hack:PS_actualState_Request
	rdf:type v-ui:DatatypePropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:actualState ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 1 ;
	v-ui:tooltip "Введите информацию о текущем статусе объекта, на который направлены изменения"@ru ;
	v-ui:tooltip "Describe current status of the facility requiring changes"@en ;
	rdfs:label "Спецификация v-s:actualState для hack:Request"@ru ;
	rdfs:label "v-s:actualState property specification for hack:Request class"@en ;
.

#-----Описание
# v-s:description
hack:PS_description_Request
	rdf:type v-ui:DatatypePropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:description ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 1 ;
	v-ui:tooltip "Подробно и максимально понятно опишите суть Вашей идеи. Помните, что идея должна быть нова, целесообразна и осуществима"@ru ;
	v-ui:tooltip "Describe the point of your idea fully in detail. Keep in mind that the idea should be new, rational and feasible"@en ;
	rdfs:label "Спецификация v-s:description для hack:Request"@ru ;
	rdfs:label "v-s:description property specification for hack:Request class"@en ;
.

#-----Тематика
#v-s:hasSector
hack:PS_hasSector_Request
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:hasSector ;
	v-ui:maxCardinality 3 ;
	v-ui:minCardinality 0 ;
	v-ui:alternativeName "Область применения"@ru ;
	v-ui:tooltip "Выберите сферы деятельности, на которые распространяется Ваша идея. Не более 3 штук."@ru ;
	v-ui:tooltip "Select functional areas which are covered by your idea"@en ;
	rdfs:label "Спецификация v-s:hasSector для hack:Request"@ru ;
	rdfs:label "v-s:hasSector property specification for hack:Request class"@en ;
.

#-----Вложение
#v-s:attachment

#-----Автор 
#v-s:author
hack:PS_author_Request
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:author ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 1 ;
	v-ui:queryPrefix "('rdf:type'=='v-s:Appointment' )"^^xsd:string ;
	v-ui:tooltip "Укажите автора идеи"@ru ;
	v-ui:tooltip "Non-required, simple value. Link to instance of Appointment class"@en ;
	v-ui:placeholder "Начните ввод и выберите значение"@ru ;
	v-ui:placeholder "Start typing and select"@en ;
	rdfs:label "Спецификация v-s:author для hack:Request"@ru ;
	rdfs:label "v-s:author property specification for hack:Request class"@en ;
.

#-----Заинтересованная сторона
#v-s:stakeholder
hack:PS_stakeholder_Request
	rdf:type v-ui:ObjectPropertySpecification ;
	v-ui:forClass hack:Request ;
	v-ui:forProperty v-s:stakeholder ;
	v-ui:maxCardinality 1 ;
	v-ui:minCardinality 1 ;
	v-ui:queryPrefix "('rdf:type' === 'v-s:Department' && 'v-s:hasChief.isExists' == 'true')"^^xsd:string ;
	v-ui:tooltip "Укажите подразделение, которому выгодна реализация Вашей инновации"@ru ;
	v-ui:tooltip "Select department which will benefit from the request"@en ;
	v-ui:placeholder "Начните ввод и выберите значение"@ru ;
	v-ui:placeholder "Start typing and select"@en ;
	rdfs:label "Спецификация v-s:stakeholder для hack:Request"@ru ;
	rdfs:label "v-s:stakeholder property specification for hack:Request class"@en ;
.

