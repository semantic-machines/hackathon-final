  v-ui:treeRoot hack:EquipmentCategory_root ;
  v-ui:treeInProperty v-s:hasParentLink ;
  v-ui:treeAllowedClass hack:EquipmentCategory ;
  v-ui:treeDisplayedProperty rdfs:label ;
.

hack:EquipmentCategory_root
